    "environement",
    "environements",
    "environements-variables",
    "environements variables",
    "4kb"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:davidroman0O/serverless-fuck-you-4kb.git"
  },
  "homepage": "https://github.com/davidroman0O/serverless-fuck-you-4kb#readme",
  "author": "David Roman",
  "devDependencies": {
    "benchmarkify": "2.1.0",
    "coveralls": "3.0.0",
    "eslint": "4.8.0",
    "jest": "^20.0.4",
    "jest-cli": "20.0.4",
    "lolex": "^2.1.3",
    "nodemon": "1.12.1",
    "npm-check": "5.4.5"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/"
